<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.groupproject.EndJoinPageController"
            prefHeight="400.0" prefWidth="600.0">

    <Label fx:id="confirmationLabel"
           layoutX="50" layoutY="50"
           text="You have successfully joined the event!" />

    <Button fx:id="returnButton"
            layoutX="50" layoutY="100"
            text="Return to Main"
            onAction="#onReturnButtonClick" />
</AnchorPane>

